<form method="post" action="{{ path('student_delete', {'id': student.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ student.id) }}">
    <div class="container text-center">
        <button class="btn btn-warning" style="margin-top: 10px;">Delete</button>
    </div>
</form>
